{
  "manifest_version": 3,
  "name": "Random McMaster Page",
  "version": "1.0.3",
  "description": "Adds a 'Random Page' link to the bottom navbar on mcmaster.com",
  "author": "kgoforth1503@gmail.com",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mcmaster.com/*"
      ],
      "js": [
        "random_page.js"
      ]
    }
  ]
}